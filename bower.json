    "bootstrap-hover-dropdown": "~2.1.3",
    "isMobile": "~0.3.9",
    "matchHeight": "~0.6.0",
    "blueimp-gallery": "~2.16.0",
    "blueimp-bootstrap-image-gallery": "~3.1.3",
    "jquery.fitvids": "FitVids#~1.1.0",
